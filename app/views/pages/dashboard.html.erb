<%= bootstrap_flash %>
<% if current_user.has_role? :admin %>
    <%= render partial: 'pages/user_dashboard/admin_dashboard' %>
<% elsif current_user.has_role? :distributor %>
    <%= render partial: 'pages/user_dashboard/distributor_dashboard' %>
<% elsif current_user.has_role? :agent %>
    <%= render partial: 'pages/user_dashboard/agent_dashboard' %>
<% else %>
    <%= render partial: 'pages/user_dashboard/client_dashboard' %>
<% end -%>