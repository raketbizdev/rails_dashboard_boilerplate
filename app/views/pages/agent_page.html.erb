<h3 class="h3 mb-2 text-gray-800">Agents</h3>
<%= bootstrap_flash %>
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <div class="d-sm-flex justify-content-between align-items-center">
      <h6 class="m-0 font-weight-bold text-primary">Client Lists</h6>
      <%= link_to "Add Agent", new_user_path, class: "btn btn-primary btn-sm d-none d-sm-inline-block" %>
    </div>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-striped table-sm" id="dataTable" width="100%" cellspacing="0">
        <thead>
          <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Phone</th>
        
          </tr>
        </thead>

        <tbody>
          <% @agents.each do |user| %>
            <tr>
              <td><%= user.profile.first_name %> <%= user.profile.last_name %></td>
              <td><%= user.email %></td>
              <td><%= user.profile.mobile_phone  %></td>
             
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>